                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_vazao_gas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/txt_parametros"
                android:layout_marginTop="25dp"
                android:text="@string/txt_vazao"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_velocidade_soldagem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_vazao_gas"
                android:layout_marginTop="25dp"
                android:text="@string/txt_velocidade_soldagem"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_velocidade_arame"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_velocidade_soldagem"
                android:layout_marginTop="23dp"
                android:text="@string/txt_velocidade_arame"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_transferencia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_velocidade_arame"
                android:layout_marginTop="25dp"
                android:text="@string/txt_transferencia"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_corrente"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_transferencia"
                android:layout_marginTop="25dp"
                android:text="@string/txt_corrente"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_tensao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_corrente"
                android:layout_marginTop="25dp"
                android:text="@string/txt_tensao"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_dbc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_tensao"
                android:layout_marginTop="25dp"
                android:text="@string/txt_dbc"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_penetracao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
