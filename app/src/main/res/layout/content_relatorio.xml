                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/id_txt_resultado_tracao_longitudinal"
                android:layout_alignTop="@+id/id_txt_tracao_transversal"
                android:background="#FFFFFF " />

            <TextView
