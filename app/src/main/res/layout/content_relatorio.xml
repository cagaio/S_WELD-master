                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/txt_resultado_penetracao"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/txt_resultado_penetracao"
                android:layout_alignTop="@+id/id_txt_vazao_gas"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/txt_resultado_velocidade_soldagem"
