                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/id_txt_regiao_intercritica"
                android:layout_alignStart="@+id/id_txt_regiao_intercritica"
                android:layout_below="@+id/id_txt_regiao_intercritica"
                android:layout_marginTop="25dp"
                android:text="@string/txt_regiao_subcritica"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_regiao_nao_modificada"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/id_txt_regiao_subcritica"
                android:layout_alignStart="@+id/id_txt_regiao_subcritica"
                android:layout_below="@+id/id_txt_regiao_subcritica"
                android:layout_marginBottom="9dp"
