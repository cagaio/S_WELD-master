                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textVieid_txt_dureza_hrc"
                android:layout_alignLeft="@+id/id_txt_resultado_tracao_transversal"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/id_txt_resultado_tracao_transversal"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/id_txt_resultado_dureza_hrc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textVieid_txt_dureza_hrc"
                android:layout_alignLeft="@+id/id_txt_resultado_tracao_transversal"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/id_txt_resultado_tracao_transversal"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textVieid_txt_dureza_hrc"
                android:layout_marginTop="35dp"
                android:background="#02eaff"
                android:text="@string/txt_micro_estrutura"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_grao_gg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView36"
                android:layout_marginTop="25dp"
                android:text="@string/txt_grao_gg"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_grao_gf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/id_txt_grao_gg"
                android:layout_alignStart="@+id/id_txt_grao_gg"
                android:layout_below="@+id/id_txt_grao_gg"
                android:layout_marginTop="25dp"
                android:text="@string/txt_grao_gf"
                android:textStyle="bold" />

            <TextView
