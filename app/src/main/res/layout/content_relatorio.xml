                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_temperatura"
                android:layout_marginTop="23dp"
                android:background="#02eaff"
                android:text="@string/txt_resistencia_materiais"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_tracao_longitudinal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_resistencia_materiais"
                android:layout_marginTop="25dp"
                android:text="@string/txt_tracao_longitudinal"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_tracao_transversal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_tracao_longitudinal"
                android:layout_marginTop="25dp"
                android:text="@string/txt_tracao_transversal"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_dureza_hb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_tracao_transversal"
                android:layout_marginTop="25dp"
