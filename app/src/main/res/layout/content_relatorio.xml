                android:layout_alignEnd="@+id/txt_resultado_vazao_gas"
                android:layout_alignLeft="@+id/txt_resultado_vazao_gas"
                android:layout_alignRight="@+id/txt_resultado_vazao_gas"
                android:layout_alignStart="@+id/txt_resultado_vazao_gas"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/txt_resultado_velocidade_arame"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/id_txt_velocidade_arame"
                android:layout_alignLeft="@+id/txt_resultado_velocidade_soldagem"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/txt_resultado_velocidade_soldagem"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/txt_resultado_transferencia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/txt_resultado_velocidade_arame"
                android:layout_alignLeft="@+id/txt_resultado_velocidade_arame"
                android:layout_alignRight="@+id/txt_resultado_velocidade_arame"
                android:layout_alignStart="@+id/txt_resultado_velocidade_arame"
                android:layout_alignTop="@+id/id_txt_transferencia"
                android:background="#FFFFFF " />

            <TextView
                android:id="@+id/txt_resultado_corrente"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/txt_resultado_transferencia"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignStart="@+id/txt_resultado_transferencia"
