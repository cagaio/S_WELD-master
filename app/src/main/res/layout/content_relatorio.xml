                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/id_txt_dureza_hb"
                android:layout_marginTop="25dp"
                android:text="@string/txt_dureza_HRC"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/id_txt_resultado_tracao_longitudinal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/id_txt_tracao_longitudinal"
                android:layout_toEndOf="@+id/id_txt_velocidade_arame"
